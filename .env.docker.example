# docker-compose environment variables

# OpenAI (필수: ai-server용, backend RAG 동기화용)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# 한국관광공사 Tour API
DATA_GO_KR_SERVICE_KEY=

# PostgreSQL (Pgvector)
# - ai-server: VectorRetriever RAG용 (docker-compose에서 ai-server에 자동 설정)
# - backend: DB는 동일 인스턴스 사용 (DB_URL로 연결)
# - ARM 환경 기본값: pgvector/pgvector:pg16 + linux/arm64
# POSTGRES_IMAGE=pgvector/pgvector:pg16
# POSTGRES_PLATFORM=linux/arm64
AI_SERVER_PORT=8081
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/questofseoul

# Spring Boot Backend (docker-compose -> backend)
SPRING_PROFILES_ACTIVE=dev
SERVER_PORT=8080
DB_URL=jdbc:postgresql://postgres:5432/questofseoul
DB_USERNAME=postgres
DB_PASSWORD=postgres
JPA_DDL_AUTO=update
FRONTEND_URL=http://localhost:3000
AI_SERVER_URL=http://ai-server:8081
AI_SERVER_ENABLED=true
APP_AUTH_ADMIN_EMAILS=
APP_AUTH_ADMIN_USER_IDS=

# OAuth Google (미설정 시 placeholder 사용, 실제 로그인 불가)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# AWS S3
AWS_S3_ENABLED=false
AWS_S3_BUCKET=
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
