{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AI Server (FastAPI)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--reload",
        "--host",
        "0.0.0.0",
        "--port",
        "8081"
      ],
      "cwd": "${workspaceFolder}/ai-server",
      "jinja": true,
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/ai-server"
      }
    },
    {
      "name": "Spring Boot (Quest of Seoul)",
      "type": "java",
      "request": "launch",
      "mainClass": "com.app.questofseoul.QuestofseoulApplication",
      "projectName": "questofseoul"
    },
    {
      "name": "Frontend (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/frontend/administration-page"
    },
    {
      "name": "Python: Test RAG",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/ai-server/scripts/test_rag.py",
      "cwd": "${workspaceFolder}/ai-server",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ],
  "compounds": []
}
