# Spring Profile
SPRING_PROFILES_ACTIVE=dev

# Server Configuration
SERVER_PORT=8080

# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/questofseoul
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_POOL_SIZE=10
DB_POOL_MIN_IDLE=5

# JPA Configuration
JPA_DDL_AUTO=update
JPA_SHOW_SQL=true

# OAuth2 Google Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# JWT
JWT_SECRET=questofseoul-dev-secret-key-at-least-256-bits-for-hs256
JWT_ACCESS_TOKEN_EXPIRATION_MS=86400000

# Swagger Configuration (Production)
SWAGGER_ENABLED=false

# AI Server
AI_SERVER_URL=http://localhost:8081
AI_SERVER_ENABLED=true

# AWS S3
AWS_S3_ENABLED=true
AWS_S3_BUCKET=your-bucket-name
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
